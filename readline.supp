#valgrind --leak-check=full --track-fds=yes --trace-children=yes --show-leak-kinds=all --suppressions=readline.supp ./minishell
{
   readline
   Memcheck:Leak
   ...
   fun:readline
}
{
   readline
   Memcheck:Leak
   ...
   fun:add_history
}
{
	ignore_usr_bin
	Memcheck:Leak
	match-leak-kinds: reachable
	fun:*
	obj:/usr/*
}
